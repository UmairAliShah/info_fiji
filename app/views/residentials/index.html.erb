<p id="notice"><%= notice %></p>

<h1>Residentials</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Gender</th>
      <th>Skype</th>
      <th>Whatsapp</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @residentials.each do |residential| %>
      <tr>
        <td><%= residential.first_name %></td>
        <td><%= residential.last_name %></td>
        <td><%= residential.gender %></td>
        <td><%= residential.skype %></td>
        <td><%= residential.whatsapp %></td>
        <td><%= residential.user %></td>
        <td><%= link_to 'Show', residential %></td>
        <td><%= link_to 'Edit', edit_residential_path(residential) %></td>
        <td><%= link_to 'Destroy', residential, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Residential', new_residential_path %>
